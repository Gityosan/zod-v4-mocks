{
  "name": "zod-v4-mocks",
  "version": "1.0.15",
  "type": "module",
  "license": "MIT",
  "private": false,
  "description": "Mock generator for zod v4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gityosan/zod-v4-mocks.git"
  },
  "author": {
    "name": "IIHARA議長",
    "url": "https://x.com/iihara_gityo"
  },
  "keywords": [
    "zod",
    "faker",
    "randexp",
    "mock",
    "generator"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "@faker-js/faker": "9.8.0",
    "es-toolkit": "1.39.5",
    "randexp": "0.5.3",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "5.5.0",
    "@types/node": "24.0.7",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "eslint": "9.39.1",
    "prettier": "3.6.1",
    "tsup": "8.3.5",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  }
}
